<script>
    //Classificação TRUE
    $(function () {
        $('#TBtable tfoot th').each(function () {
            var title = $(this).text();
            $(this).html('<div class="input-icon-group"> <input type="text" placeholder="' + title + '"/> <div class="input-icon-search"> <i class="fas fa-search"></i> </div></div>');
